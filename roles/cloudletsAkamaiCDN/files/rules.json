               },
               {
                   "type": "erMatchRule",
                   "id":0,
                   "name": "RegexRedirect",
                    "start": 0,
                    "end": 0,  
                    "matches": [
                       {
                           "matchValue": "https", 
                           "matchOperator": "equals",
                           "negate": false,
                           "caseSensitive": false, 
                           "matchType": "protocol"
                       }, 
                       { 
                           "matchValue": "akamaipremium.dev.technology.ca.gov", 
                           "matchOperator": "equals", 
                           "negate": false, 
                           "caseSensitive": false, 
